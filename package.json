{
  "name": "unbrowse-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "bun test packages/plugin/test/*.test.ts",
    "test:e2e": "E2E_BACKEND_START=docker bun test packages/plugin/test/e2e",
    "test:oct": "bash packages/plugin/scripts/test-oct.sh",
    "build": "cd packages/plugin && npx tsc",
    "build:web": "cd packages/web && pnpm build",
    "typecheck": "cd packages/plugin && npx tsc --noEmit"
  },
  "license": "Apache-2.0",
  "description": "Open protocol for agent web access. Capture, package, and replay internal web APIs."
}