{
  "name": "@getfoundry/unbrowse-openclaw",
  "version": "0.5.4",
  "type": "module",
  "main": "index.ts",
  "description": "Open source API reverse engineering for OpenClaw. Capture any API and turn it into monetizable skills for AI agents.",
  "files": [
    "index.ts",
    "src/",
    "hooks/",
    "openclaw.plugin.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lekt9/unbrowse-openclaw"
  },
  "keywords": [
    "openclaw",
    "clawdbot",
    "ai-agent",
    "api-reverse-engineering",
    "browser-automation",
    "skill-marketplace",
    "x402",
    "solana",
    "usdc"
  ],
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "hooks": [
      "./hooks/auto-discover"
    ]
  },
  "clawdbot": {
    "extensions": [
      "./index.ts"
    ],
    "hooks": [
      "./hooks/auto-discover"
    ]
  },
  "moltbot": {
    "extensions": [
      "./index.ts"
    ],
    "hooks": [
      "./hooks/auto-discover"
    ]
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.95.0",
    "bs58": "^5.0.0",
    "playwright": "^1.58.0",
    "tweetnacl": "^1.0.3",
    "ws": "^8.18.3"
  },
  "peerDependencies": {
    "openclaw": "*",
    "clawdbot": "*",
    "moltbot": "*"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    },
    "clawdbot": {
      "optional": true
    },
    "moltbot": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}
