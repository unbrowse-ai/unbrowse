{
  "name": "@getfoundry/unbrowse-openclaw",
  "version": "0.4.2",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Your agent is useless without an API. Unbrowse fixes that. Capture internal APIs from any website and skip the slow browser. 100x faster web access for AI agents.",
  "homepage": "https://unbrowse.ai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lekt9/unbrowse-openclaw"
  },
  "keywords": [
    "openclaw",
    "clawdbot",
    "ai-agent",
    "api-reverse-engineering",
    "browser-automation",
    "skill-marketplace",
    "x402",
    "solana"
  ],
  "files": [
    "dist/",
    "src/",
    "hooks/",
    "openclaw.plugin.json",
    "clawdbot.plugin.json"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ],
    "hooks": [
      "./hooks/auto-discover"
    ]
  },
  "clawdbot": {
    "extensions": [
      "./dist/index.js"
    ],
    "hooks": [
      "./hooks/auto-discover"
    ]
  },
  "moltbot": {
    "extensions": [
      "./dist/index.js"
    ],
    "hooks": [
      "./hooks/auto-discover"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "openclaw": "*",
    "clawdbot": "*",
    "moltbot": "*",
    "playwright": "^1.40.0"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    },
    "clawdbot": {
      "optional": true
    },
    "moltbot": {
      "optional": true
    },
    "playwright": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}
