{
  "name": "@getfoundry/unbrowse-openclaw",
  "version": "0.7.9",
  "license": "AGPL-3.0-only",
  "type": "module",
  "main": "dist/index.js",
  "description": "Open source API reverse engineering for OpenClaw. Capture any API and turn it into monetizable skills for AI agents.",
  "files": [
    "dist/",
    "hooks/",
    "openclaw.plugin.json",
    "clawdbot.plugin.json",
    "README.md",
    "skills/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lekt9/unbrowse-openclaw",
    "directory": "packages/plugin"
  },
  "keywords": [
    "openclaw",
    "clawdbot",
    "ai-agent",
    "api-reverse-engineering",
    "browser-automation",
    "skill-marketplace",
    "x402",
    "solana",
    "usdc"
  ],
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ],
    "hooks": [
      "./hooks/auto-discover"
    ]
  },
  "clawdbot": {
    "extensions": [
      "./dist/index.js"
    ],
    "hooks": [
      "./hooks/auto-discover"
    ]
  },
  "moltbot": {
    "extensions": [
      "./dist/index.js"
    ],
    "hooks": [
      "./hooks/auto-discover"
    ]
  },
  "dependencies": {
    "@getfoundry/unbrowse-core": "0.7.9",
    "playwright-core": "^1.58.2"
  },
  "bundledDependencies": [
    "@getfoundry/unbrowse-core"
  ],
  "peerDependencies": {
    "clawdbot": "*",
    "moltbot": "*",
    "openclaw": "*"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    },
    "clawdbot": {
      "optional": true
    },
    "moltbot": {
      "optional": true
    }
  },
  "scripts": {
    "test": "bun test test/*.test.ts",
    "test:e2e": "E2E_BACKEND_START=docker bun test test/e2e",
    "test:oct": "bash scripts/test-oct.sh",
    "test:oct:docker": "bash scripts/test-oct-docker.sh",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && cp ../../README.md ."
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  },
  "types": "dist/index.d.ts"
}
