{
  "name": "@getfoundry/unbrowse-native",
  "version": "0.1.0",
  "description": "Native Rust addon for Unbrowse - closed-source HAR parsing and auth extraction",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/lekt9/unbrowse-openclaw",
    "directory": "native"
  },
  "napi": {
    "name": "unbrowse-native",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-apple-darwin",
        "aarch64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "x86_64-pc-windows-msvc",
        "aarch64-unknown-linux-gnu"
      ]
    }
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts",
    "version": "napi version"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ],
  "optionalDependencies": {
    "@getfoundry/unbrowse-native-darwin-x64": "0.1.0",
    "@getfoundry/unbrowse-native-darwin-arm64": "0.1.0",
    "@getfoundry/unbrowse-native-linux-x64-gnu": "0.1.0",
    "@getfoundry/unbrowse-native-win32-x64-msvc": "0.1.0",
    "@getfoundry/unbrowse-native-linux-arm64-gnu": "0.1.0"
  }
}
